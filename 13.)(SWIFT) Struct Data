func main(){
    struct DataPribadi{
        var nama  = "Ikko Cahya Awinata"
        var nim    = "22051204038"
        var JenisKelamin = "perempuan"
        var alamat = "jalan sampoerna, Surabaya"
    }

    let m = DataPribadi()
    print(" Nama          = \(m.nama)")
    print(" NIM           = \(m.nim)")
    print(" Jenis Kelamin = \(m.JenisKelamin)")
    print(" Jenis Kelamin = \(m.alamat)")
}

main()
