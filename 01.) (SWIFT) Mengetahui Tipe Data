import Foundation

func judul() {
    print("=====================================================")
    print("              SELAMAT DATANG DI PROGRAM              ")
    print("                MENGETAHUI TIPE DATA                 ")
    print("=====================================================")
}

func main() {
    judul()

    print("1.) TIPE DATA INTEGER")
    let dataInteger: Int = 0
    print("Data     : \(dataInteger)")
    print("Bertipe  : \(type(of: dataInteger))\n")

    print("2.) TIPE DATA FLOAT")
    let dataFloat: Double = 6.08
    print("Data     : \(dataFloat)")
    print("Bertipe  : \(type(of: dataFloat))\n")

    print("3.) TIPE DATA STRING")
    let dataString: String = "Ikko Cahya Awinata"
    print("Data     : \(dataString)")
    print("Bertipe  : \(type(of: dataString))\n")

    print("4.) TIPE DATA BOOLEAN")
    let dataBool: Bool = true
    print("Data     : \(dataBool)")
    print("Bertipe  : \(type(of: dataBool))\n")

    print("5.) TIPE DATA CHAR")
    let dataChar: Character = "F"
    print("Data     : \(dataChar)")
    print("Bertipe  : \(type(of: dataChar))\n")

    print("6.) TIPE DATA ARRAY")
    let dataArray: [Int] = [1, 2, 3, 4, 5]
    print("Data     : \(dataArray)")
    print("Bertipe  : \(type(of: dataArray))\n")
}

main()
