        pil = pilInt
    }
    print(" \n ")

    switch pil{
        case 1:
        print ("------ 1.) KM -> HM -------- ")
        print("Masukkan angka = ")
        n = Int(readLine()!) ?? 0
        rumus = n * 10 
        print(" \(n) * 10 = \(rumus) HM")



        case 2:
        print ("------ 2.) KM -> DAM -------- ")
