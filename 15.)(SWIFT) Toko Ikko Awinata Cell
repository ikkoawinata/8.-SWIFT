    print("5. ||\t cetak foto 3x4 (1 foto get 6 pcs\t || Rp.\t\t\t\t\t   7.000||")
    print("6. ||\t cetak foto 2x3 (1 foto get 6 pcs\t || Rp.\t\t\t\t\t   7.000||")
    print("7. ||\t Pengetikan / lembar           \t\t || Rp.\t\t\t\t\t   5.000||")
